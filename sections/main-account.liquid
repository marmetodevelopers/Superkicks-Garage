      <button class="tablinks" id="tablinks" onclick="openTab(event, 'address')">
        <svg class="tabs-svg" id="address-m-svg" width="14" height="15" viewBox="0 0 14 15" xmlns="http://www.w3.org/2000/svg">
          <path d="M9.25 7.25V2.75L7 0.5L4.75 2.75V4.25H0.25V14.75H13.75V7.25H9.25ZM3.25 13.25H1.75V11.75H3.25V13.25ZM3.25 10.25H1.75V8.75H3.25V10.25ZM3.25 7.25H1.75V5.75H3.25V7.25ZM7.75 13.25H6.25V11.75H7.75V13.25ZM7.75 10.25H6.25V8.75H7.75V10.25ZM7.75 7.25H6.25V5.75H7.75V7.25ZM7.75 4.25H6.25V2.75H7.75V4.25ZM12.25 13.25H10.75V11.75H12.25V13.25ZM12.25 10.25H10.75V8.75H12.25V10.25Z" fill="#000" />
        </svg>
        <p>Manage Address</p>
      </button>
    </div>
    <div id="mobile_tabs_content">
      <div id="myprofile" class="tabcontent">
        <!-- Content for My Profile tab goes here for mobile -->
        <div class="profile_container_mobile">
          <div>
            <p class="customer_name-mobile">Hi, {{ customer.first_name }}</p>
          </div>
          <div class="profile__name">
            <p class="customer_name-tittle">Name:</p>
            <div class="customer_name-field">
              <p class="customer__field-name">{{ customer.first_name }}</p>
            </div>
          </div>
          <div class="profile__email">
            <p class="customer_email-tittle">Email:</p>
            <div class="customer_name-field">
              <p class="customer__field-mail">{{ customer.email }}</p>
            </div>
          </div>
          <div class="default_address-card">
            {% assign default_address = '' %} 
            {%- for address in customer.addresses -%} 
            {%- if address == customer.default_address -%} 
            <h2 class="customer_default_title">Default Address:</h2>
            {% assign default_address = address %} 
            {%- endif -%}
            {% endfor %}          
            <div class="default_card-cotent">
              <div class="default_card-wrapper">
              <span class="customer-name f-w-blod">{{ default_address.name }}</span>
             <div class="edit_default-address">Edit</div>
              </div>
              {{ default_address | format_address | remove: default_address.name | replace_first: '<br>', '' }}
              <p class="customer_phone">{{ default_address.phone }}</p>
            </div>
          </div>
        </div>
        <div class="logout_url_mobile">
          <a href="{{ routes.account_logout_url }}" class="logout__link_mobile">
            <div class="logout_card-mobile">
              <svg width="20" height="18" viewBox="0 0 16 15" fill="#fff" xmlns="http://www.w3.org/2000/svg">
                <path d="M11.75 3.75L10.6925 4.8075L12.6275 6.75H5V8.25H12.6275L10.6925 10.185L11.75 11.25L15.5 7.5L11.75 3.75ZM2 2.25H8V0.75H2C1.175 0.75 0.5 1.425 0.5 2.25V12.75C0.5 13.575 1.175 14.25 2 14.25H8V12.75H2V2.25Z" fill="#fff" />
              </svg>
              <p class="logout_text">{{ 'customer.log_out' | t }}</p>
            </div>
          </a>
        </div>
      </div>
      <div id="myorders" class="tabcontent">
        <div class="recent_orders_mobile" id="my_orders_container">
          {%- for order in customer.orders limit: 8-%} 
          <div class="my_orders_container">
            <div class="recent_order_info">
              <div class="recent_order_id">
                <p class="my_order_text">Order Id</p>
                <span class="myorder_detail_opener__mobile recent_order_text_span" data-orderName="{{ order.name }}" href="{{ order.customer_url }}" aria-label="{{ 'customer.orders.order_number_link' | t: number: order.name }}">
                {{ order.name }}
                </span>
              </div>
              <div class="order_details">
                <div>
                  <p class="my_order_text"> Date <span class="recent_order_span"> {{order.created_at | time_tag: format: 'date'}} </span>
                  </p>
                </div>
                <div class="payment_status">
                  <p class="my_order_text"> Payment Status <span class="recent_order_span">{{ order.financial_status_label }}</span>
                  </p>
                </div>
                <div class="total">
                  <p class="my_order_text"> Total <span class="recent_order_span">{{ order.total_net_amount | money_with_currency }}</span>
                  </p>
                </div>
                <div class="order_status">
                  <p class="my_order_text track">Order Status</p>
                  <span class="track_span">
                 {%- assign order_date = order.created_at | time_tag: format: 'date' -%}
                 {% assign date = "May 01,2023" | time_tag: format: 'date' %}
                  {% if order_date < date %}
                    <p>Deliverd</p>
                  {% else %}
                    {% if order.cancelled %} 
                    <p class="tracking-status">Cancelled</p>
                    {% else %} 
                    {% if order.fulfillment_status_label == 'Fulfilled' %} 
                    {% for line_item in order.line_items %} 
                    {% if forloop.first %}
                    {% if order.financial_status_label contains 'Partially' %}
                      <a href=" {{ order.customer_url }}" target="_self" aria-label="{{ 'customer.orders.order_number_link' | t: number: order.name }}"> Track Order </a> 
                    {% else %} 
                      <a href="{{line_item.fulfillment.tracking_url}}" target="_blank" aria-label="{{ 'customer.orders.order_number_link' | t: number: order.name }}"> Track Order </a> 
                    {% endif %} 
                    {% endif %}
                    {% endfor %} 
                    {% elsif order.fulfillment_status_label == 'Unfulfilled' -%} 
                    <p class="tracking-status">Order Processing</p>
                    {%- else -%} 
                    {{- order.fulfillment_status_label -}} 
                    {%- endif %} 
                    {% endif %} 
                   {% endif %}
                  </span>
                </div>
              </div>
            </div>
          </div>
          {% endfor %}
        </div>
        <!-- ORDERS PAGE FOR MOBILE -->
        <div class="order__page_mobile" id="my_order_detail_mobile">
          {% for order in customer.orders %} 
          <div class="my-orderDetail" data-order-name-matcher="{{ order.name }}"> 
          {% render 'my-orders-snippet_mobile', order: order %} </div>
          {% endfor %} 
        </div>
        <div id="viewAllMyOrders"></div>
      </div>
      <div id="address" class="tabcontent">
        {% render 'manage-address-snippet-mobile' %}
      </div>
    </div>
  </div>
  <!-- TABS SWITCH FOR MOBILE DESION -->
  <div class="tabs_desktop">
    <div class="tab">
      <div class="customer__title">
        <p class="customer_name">Hi, {{ customer.first_name }}</p>
      </div>
