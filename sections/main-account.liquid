{{ 'account.css' | asset_url | stylesheet_tag }}
<script src="{{ 'account.js' | asset_url }}" defer></script>
<script src="{{ 'customer.js' | asset_url }}" defer></script>

<div class=" page-width customer account  section-{{ section.id }}-padding">
  <div class="home_page-icon">
    <a href="{{ routes.root_url }}"> {% render 'home-icon' %} </a>
    <p class="account_login-name">>Account</p>
  </div>
  <!-- TABS SWITCH FOR MOBILE DESION -->
  <div class="tabs">         
    <div class="mobile_tabs">        
      <button class="tablinks active myprofile" id="tablinks" onclick="openTab(event, 'myprofile')">
