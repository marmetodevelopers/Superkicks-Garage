    <div id="MyProfile" class="tabcontent">
      <div class="MyProfile_content" id="MyProfile_content">
        <div class="profile_description">
          <div class="profile_title">
            <svg width="19" height="20" viewBox="0 0 19 20" fill="#8D0349;" xmlns="http://www.w3.org/2000/svg">
              <path d="M9.5 0.5C4.256 0.5 0 4.756 0 10C0 15.244 4.256 19.5 9.5 19.5C14.744 19.5 19 15.244 19 10C19 4.756 14.744 0.5 9.5 0.5ZM9.5 3.35C11.077 3.35 12.35 4.623 12.35 6.2C12.35 7.777 11.077 9.05 9.5 9.05C7.923 9.05 6.65 7.777 6.65 6.2C6.65 4.623 7.923 3.35 9.5 3.35ZM9.5 16.84C7.125 16.84 5.0255 15.624 3.8 13.781C3.8285 11.8905 7.6 10.855 9.5 10.855C11.3905 10.855 15.1715 11.8905 15.2 13.781C13.9745 15.624 11.875 16.84 9.5 16.84Z" fill="#223263" />
            </svg>
            <p>My Profile</p>
          </div>
        </div>
        <div class="profile__name">
          <p class="customer_name-tittle">Name:</p>
          <div class="customer_name-field">
            <p class="customer__field-name">{{ customer.first_name }}</p>
          </div>
        </div>
        <div class="profile__email">
          <p class="customer_email-tittle">Email:</p>
          <div class="customer_name-field">
            <p class="customer__field-mail">{{ customer.email }}</p>
          </div>
        </div>
        <div class="default_address-card">
          {% assign default_address = '' %} 
          {%- for address in customer.addresses -%} 
          {%- if address == customer.default_address -%} 
          <h2 class="customer_default_title">Default Address:</h2>
          {% assign default_address = address %} 
          {%- endif -%} 
          {% endfor %} 
          <div class="default_card-cotent">
             <div class="default_card-wrapper">
              <span class="customer-name f-w-blod">{{ default_address.name }}</span>
                <div class="edit_default-address">Edit</div>
              </div>
            {{ default_address | format_address | remove: default_address.name | replace_first: '<br>', '' }}
            <p class="customer_phone">{{ default_address.phone }}</p>
          </div>
        </div>
      </div>
      <div class="recent_orders-list" id="recent_orders-list">
        <div class="my_orders_title">
          <svg width="19" height="20" viewBox="0 0 28 30" fill="none" xmlns="http://www.w3.org/2000/svg">
