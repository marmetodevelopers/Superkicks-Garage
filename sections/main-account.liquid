              <clipPath id="clip0_6658_28822">
                <rect width="28" height="30" fill="white" />
              </clipPath>
            </defs>
          </svg>
          <p>My Orders</p>
        </div>
      </div>
      <div class="order__page_dektop" id="main_order_detail_desktop">
        {% for order in customer.orders %} 
        <div class="my-orderDetail" data-order-name-matcher="{{ order.name }}"> 
          {% render 'my-orders-snippet_desktop', order: order %} 
        </div>
        {% endfor %} 
      </div>
      <div id="MyordersDesktop"></div>
    </div>
    <div id="ManageAddress" class="tabcontent"> 
      {% render 'manage-address-snippet-desktop' %} 
    </div>
  </div>
